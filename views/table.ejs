<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/style.css">
  <title>Table Data</title>
</head>
<body>
  <%- include ('./partials/navbar.ejs') %>
  <div id="table">
    <div id="table-wrap">
      <table style="width: 100%;">
        <tr>
          <th>No</th>
          <th>NIM</th>
          <th>Nama</th>
          <th>Jurusan</th>
          <th>Aksi</th>
        </tr>
        <% datas.map( (data, index) => { %>
          <tr>
            <td><%= index + 1 %></td>
            <td><%= data.nim %></td>
            <td><%= data.nama %></td>
            <td><%= data.jurusan %></td>
            <td>
              <a class="aksi edit" href="/table/edit/<%= data.id %>">Edit</a>
              <a class="aksi hapus" href="/post">Hapus</a>
            </td>
          </tr>
        <% }); %>
      </table>
    </div>
  </div>
</body>
</html>